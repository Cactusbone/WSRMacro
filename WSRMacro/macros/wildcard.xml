<grammar version="1.0" xml:lang="fr-FR" mode="voice" root="ruleWildcard" xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">
  <rule id="ruleWildcard" scope="public">
    <example>Sarah Recherche * sur Wikipedia</example>
    <tag>out.action=new Object(); </tag>

    <item weight="2">Sarah recherche</item>
    <ruleref special="GARBAGE" />
    <item>sur wikipedia</item>

      <tag>out.action._attributes.uri="http://127.0.0.1:8080/sarah/wildcard";</tag>
    <tag>out.action._attributes.dictation="2";
    out.action._attributes.threashold="0.50";</tag>
  </rule>
</grammar>
